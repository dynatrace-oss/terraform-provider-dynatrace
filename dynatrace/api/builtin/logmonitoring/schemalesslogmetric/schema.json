{
	"allowedScopes": [
		"environment"
	],
	"constraints": [
		{
			"customMessage": "Measure cannot be changed, please create a new metric instead.",
			"customValidatorId": "measure-not-changed-validator",
			"type": "CUSTOM_VALIDATOR_REF"
		}
	],
	"description": "With [log metrics](https://dt-url.net/ngn07i6), you can use queries to create metrics from logs data for dashboarding, analysis, and custom alerting.\n\n Note:\n- Newly-defined log metrics are available only for log data ingested after [metric creation](https://dt-url.net/iy23x1d).\n- Log metrics consume DDUs.\n\nFor complete details on pricing, visit [DDUs for custom metrics](https://dt-url.net/vg43xi8).",
	"displayName": "Log metrics",
	"documentation": "",
	"dynatrace": "1",
	"enums": {
		"Measure": {
			"description": "",
			"displayName": "Measure",
			"documentation": "",
			"items": [
				{
					"displayName": "Occurrence of logs records",
					"value": "OCCURRENCE"
				},
				{
					"displayName": "Attribute value",
					"value": "ATTRIBUTE"
				}
			],
			"type": "enum"
		}
	},
	"maxObjects": 100,
	"metadata": {
		"addItemButton": "Add log metric"
	},
	"multiObject": true,
	"ordered": false,
	"properties": {
		"dimensions": {
			"description": "",
			"displayName": "",
			"documentation": "",
