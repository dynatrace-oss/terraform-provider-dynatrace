					"value": "LESS_THAN_OR_EQUAL_TO"
				},
				{
					"displayName": "greater than",
					"value": "GREATER_THAN"
				},
				{
					"displayName": "greater than or equal to",
					"value": "GREATER_THAN_OR_EQUAL_TO"
				},
				{
					"displayName": "in",
					"value": "IN"
				},
				{
					"displayName": "starts with",
					"value": "STARTS_WITH"
				}
			],
			"type": "enum"
		},
		"ValueType": {
			"description": "",
			"displayName": "Value Type",
			"documentation": "",
			"items": [
