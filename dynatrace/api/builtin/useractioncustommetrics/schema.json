	"maxObjects": 100,
	"multiObject": true,
	"ordered": false,
	"properties": {
		"dimensions": {
			"constraints": [
				{
					"customMessage": "The dimensions must be unique (case insensitive)",
					"customValidatorId": "unique-case-insensitive-dimension-validator",
					"skipAsyncValidation": false,
					"type": "CUSTOM_VALIDATOR_REF"
				},
				{
					"customMessage": "The metric cannot contain more than 2 high cardinality dimensions",
					"customValidatorId": "high-cardinality-dimension-validator",
					"skipAsyncValidation": false,
					"type": "CUSTOM_VALIDATOR_REF"
				}
			],
			"description": "Defines the fields that are used as dimensions. A dimension is a collection of reference information about a metric data point that is of interest to your business. Dimensions are parameters like \"application\", \"type\", \"apdexCategory\". For example, using \"type\" as a dimension allows you to split chart data based on the user action type.",
			"displayName": "Add a dimension",
			"documentation": "",
			"items": {
				"constraints": [
					{
						"maxLength": 100,
						"minLength": 1,
						"type": "LENGTH"
					},
					{
						"customMessage": "Field must be a valid USQL one",
						"customValidatorId": "field-name-validator",
						"skipAsyncValidation": false,
						"type": "CUSTOM_VALIDATOR_REF"
					},
					{
						"customMessage": "Field is not a valid dimension. Only non-numeric fields with low cardinality are allowed as dimension",
						"customValidatorId": "dimension-field-validator",
						"skipAsyncValidation": false,
						"type": "CUSTOM_VALIDATOR_REF"
					}
				],
				"description": "",
				"displayName": "",
				"documentation": "",
				"type": "text"
			},
			"maxObjects": 4,
			"metadata": {
				"addItemButton": "Add dimension"
			},
			"minObjects": 0,
			"modificationPolicy": "DEFAULT",
			"nullable": false,
			"type": "list"
		},
		"enabled": {
			"default": true,
			"description": "",
			"displayName": "Enable custom metric",
			"documentation": "",
			"maxObjects": 1,
			"modificationPolicy": "DEFAULT",
			"nullable": false,
			"type": "boolean"
		},
		"filters": {
			"constraints": [
				{
					"customMessage": "The filters must be unique (case insensitive)",
					"customValidatorId": "unique-case-insensitive-filter-validator",
					"skipAsyncValidation": false,
					"type": "CUSTOM_VALIDATOR_REF"
				}
			],
			"description": "Defines the filters for the user action. Filters apply at the moment of extracting the data and only sessions that satisfy the filtering criteria will be used to extract the custom metrics. You will not be able to modify these filters in the metric data explorer. For example, using \"type equals Xhr\" will give you only data from xhr actions, while forcing the rest of user actions of different types to be ignored.",
			"displayName": "Add a filter",
			"documentation": "",
			"items": {
				"description": "",
				"displayName": "",
				"documentation": "",
				"type": {
					"$ref": "#/types/Filter"
				}
			},
			"maxObjects": 10,
			"metadata": {
				"addItemButton": "Add filter"
			},
			"minObjects": 0,
			"modificationPolicy": "DEFAULT",
			"nullable": false,
			"type": "list"
		},
		"metricKey": {
			"constraints": [
				{
					"maxLength": 200,
					"minLength": 1,
					"type": "LENGTH"
				},
				{
					"customMessage": "Metric key must start with 'uacm.' prefix and followed by a non-empty identifier.",
					"pattern": "^uacm\\..+$",
					"type": "PATTERN"
				},
				{
					"customMessage": "After the prefix, the metric key may contain only alphanumeric characters and underscores ('_'). Characters must be from the Latin alphabet, with no diacritics; characters such as 'รถ' are not allowed.",
					"pattern": "^[^.]+[\\.]?[A-Za-z0-9_]*?$",
					"type": "PATTERN"
				}
			],
			"default": "",
			"description": "",
			"displayName": "Metric key",
			"documentation": "",
			"maxObjects": 1,
			"metadata": {
				"placeholder": "uacm."
			},
			"modificationPolicy": "DEFAULT",
			"nullable": false,
			"type": "text"
		},
		"value": {
			"description": "Defines the type of value to be extracted from the user action. When using **user action counter**, the number of user actions is counted (similar to count(*) when using USQL). When using **user action field value**, the value of a user action field is extracted.",
			"displayName": "Value type to be extracted",
			"documentation": "",
			"maxObjects": 1,
			"modificationPolicy": "NEVER",
			"nullable": false,
			"type": {
				"$ref": "#/types/MetricValue"
			}
		}
	},
	"schemaConstraints": [
		{
			"customMessage": "The metric key must be unique (case insensitive)",
			"customValidatorId": "unique-case-insensitive-metric-key-validator",
			"skipAsyncValidation": false,
			"type": "CUSTOM_VALIDATOR_REF"
		}
	],
